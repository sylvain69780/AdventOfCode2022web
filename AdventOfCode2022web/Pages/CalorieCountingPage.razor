@page "/CalorieCountingPage"
@using AdventOfCode2022Solutions.PuzzleSolutions.CalorieCounting
<PuzzleSolutionController PuzzleSolver="@_solution">
    @if (context.PuzzleOutput is not null && context.Settings is not null)
    {
        <CalorieCountingVisualization Step=@(context.PuzzleOutput.Step) Settings="@context.Settings" Info="@((CalorieCountingInfo?)context.PuzzleOutput?.Info)"/>
        <DefaultVisualization PuzzleOutput="@context.PuzzleOutput" />
    }

</PuzzleSolutionController>
@code {
    CalorieCountingSolution _solution = new();
}
